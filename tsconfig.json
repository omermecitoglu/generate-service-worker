{
  "compilerOptions": {
    "outDir": "./bin",
    "target": "es5",
    "lib": ["webworker"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": false,
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"],
}
